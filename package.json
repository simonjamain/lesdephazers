	"devDependencies": {
		"browser-sync": "^2.26.12",
		"del-cli": "^3.0.1",
		"npm-run-all": "^4.1.5",
		"parcel": "^1.12.4",
		"purgecss": "^2.3.0",
		"sass": "^1.26.10"
	},
