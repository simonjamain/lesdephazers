{
  "name": "elConfitours.gamejam.local",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "live": "mkdir -p dist && browser-sync start --server dist --files dist",
    "watch": "mkdir -p dist && parcel watch src/index.html --out-dir ./dist/",
    "dev": "npm run watch & npm run live",
    "purge": "purgecss --css dist/*.css --content 'dist/*.html','dist/*.js' --output dist/",
    "build": "npm run clean && mkdir -p dist && parcel build --no-source-maps src/index.html --out-dir dist/ && npm run purge",
    "clean": "rm -rf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/simonjamain/lesdephazers.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/simonjamain/lesdephazers/issues"
  },
  "homepage": "https://github.com/simonjamain/lesdephazers#readme",
  "devDependencies": {
    "browser-sync": "^2.26.12",
    "parcel": "^1.12.4",
    "purgecss": "^2.3.0",
    "sass": "^1.26.10"
  },
  "dependencies": {
    "phaser": "^3.24.1"
  },
  "description": ""
}
