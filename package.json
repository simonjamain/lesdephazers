{
	"name": "elConfitours.gamejam.local",
	"version": "1.0.0",
	"main": "index.js",
	"scripts": {
		"browserSync": "browser-sync start --server dist --files dist",
		"live": "npm-run-all clean browserSync",
		"watch": "parcel watch src/index.html --out-dir ./dist",
		"dev": "npm-run-all -p watch live",
		"parcelBuild": "parcel build --no-source-maps src/index.html --out-dir ./dist",
		"build": "npm-run-all clean parcelBuild",
		"clean": "del dist/**/*"
	},
